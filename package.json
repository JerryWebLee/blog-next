{
  "name": "next-app-template",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"./**/*.+(ts|tsx)\"",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts --port=4984",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed:tags": "npx tsx scripts/seed-tags.ts",
    "test:api": "npx tsx scripts/test-api-simple.ts",
    "test:api:tags": "npx tsx scripts/test-tags-simple.ts",
    "test:db:env": "npx tsx scripts/setup-env.ts",
    "test:db:connect": "npx tsx scripts/simple-db-test.ts",
    "format": "prettier . --write",
    "lint:fix": "eslint \"./**/*.+(ts|tsx)\" --fix",
    "format:check": "prettier . --check",
    "test:db:connection": "node scripts/test-db-connection.js"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@heroui/button": "2.2.25",
    "@heroui/code": "2.2.19",
